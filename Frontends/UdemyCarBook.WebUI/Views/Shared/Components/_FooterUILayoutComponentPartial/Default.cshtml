@model List<ResultFooterAddressDto>



<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js" integrity="sha512-v2CJ7UaYy4JwqLDIrZUI/4hqeoQieOmAZNXBeQyjo21dadnwR+8ZaIJVT8EE2iyI61OV8e6M8PP2/4hpQINQ/g==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script>
    $(document).ready(function () {
        $('#btn1').click(function () {
            swal("Şartlar & Koşullar", "Araba kiralama sürecine dair tüm hizmet koşullarına rezervasyonun fiziki olarak gerçekleştirileceği ofislerimizde yer alan anlaşma dökümanlarından ulaşabilirsiniz. Bütün sürecin hasar,ceza,geçişler(hgs,ogs) ile ilgili detaylar yer almaktadır. Keyifli Yolculuklar dileriz");
        });
        $('#btn2').click(function () {
            swal("Güvenlik", "CarBook web sitemiz SHA256 Şifreleme tekniği ile şifrelenmiştir. Sistemimizde JWT doğrulaması yapılmaktadır. Kullanıcıların ödeme yaptıkları kart bilgileri kesinlikle saklanmamaktadır.");
        });
        $('#btn3').click(function () {
            swal("Çerez Politikası", "Sizlere daha iyi hizmet sunabilmek için tarayıcı çerezlerini kullanmaktayız.");
        });
        $('#btn4').click(function () {
            swal("Sıkça Sorulan Sorular", "Sistem ile ilgili sık sorulan sorular yer alır");
        });
        $('#btn5').click(function () {
            swal("Ödeme Yöntemleri", "Ödeme Yöntemleri yalnızca fiziki lokasyonlarımızda yapılmaktadır.");
        });
        $('#btn6').click(function () {
            swal("Nasıl Çalışır?", "1-Aracınızı Kiralayın \n 2-Mailinizi Kontrol Edin \n 3-Ofisimize Gelin");
        });
    });
    

</script>

@foreach (var item in Model)
{
    <footer class="ftco-footer ftco-bg-dark ftco-section">
        <div class="container">
            <div class="row mb-5">
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4">
                        <h2 class="ftco-heading-2"><a href="#" class="logo">Car<span>book</span></a></h2>
                        <p>@item.Description</p>
                        <ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-5">
                            <li class="ftco-animate"><a href="#"><span class="icon-twitter"></span></a></li>
                            <li class="ftco-animate"><a href="#"><span class="icon-facebook"></span></a></li>
                            <li class="ftco-animate"><a href="#"><span class="icon-instagram"></span></a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4 ml-md-5">
                        <h2 class="ftco-heading-2">Bilgi</h2>
                        <ul class="list-unstyled">
                            <li><a href="/About/Index/" class="py-2 d-block">Hakkımızda</a></li>
                            <li><a href="/Service/Index/" class="py-2 d-block">Hizmetlerimiz</a></li>
                            <li><a id="btn1" style="cursor:pointer;" class="py-2 d-block">Şartlar & Koşullar </a></li>
                            <li><a id="btn2" style="cursor:pointer;" class="py-2 d-block">Güvenlik</a></li>
                            <li><a id="btn3" style="cursor:pointer;" class="py-2 d-block">Çerez Politikası</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4">
                        <h2 class="ftco-heading-2">Müşteri Destek</h2>
                        <ul class="list-unstyled">
                            <li><a id="btn4" style="cursor:pointer;" class="py-2 d-block">Sıkça Sorulanlar</a></li>
                            <li><a id="btn5" style="cursor:pointer;" class="py-2 d-block">Ödeme Yöntemleri</a></li>
                            <li><a href="/CarPricing/Index/" class="py-2 d-block">Kiralama</a></li>
                            <li><a id="btn6" style="cursor:pointer;" class="py-2 d-block">Nasıl Çalışır?</a></li>
                            <li><a href="/Contact/Index/" class="py-2 d-block">Bize Ulaşın</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4">
                        <h2 class="ftco-heading-2">Bir sorunuz mu var?</h2>
                        <div class="block-23 mb-3">
                            <ul>
                                <li><span class="icon icon-map-marker"></span><span class="text">@item.Address</span></li>
                                <li><a href="#"><span class="icon icon-phone"></span><span class="text">@item.Phone</span></a></li>
                                <li><a href="#"><span class="icon icon-envelope"></span><span class="text">@item.Email</span></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 text-center">

                    <p>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        Copyright &copy;<script>document.write(new Date().getFullYear());</script> Tüm Hakları Saklıdır | This template is made with <i class="icon-heart color-danger" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
                        <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    </p>
                </div>
            </div>
        </div>
    </footer>
}
